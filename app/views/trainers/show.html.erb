<h3>Trainer</h3>
<p>Name: <%= @trainer.name if @trainer.name %></p>
<p>Email: <%= @trainer.email if @trainer.email %></p>
<p> Pokemons: </p>
<% @trainer.pokemons.each do |p| %> <!-- Iterates through each user under the variable u -->
  <p> Name: <%= p.name %> , Level: <%= p.level %> , Health: <%= p.health%> 
  	  <%= button_to "Damage!", damage_path(id: p,trainer: @trainer.id), :class => "button medium", :method => :patch %>
  </p>
<% end %>
